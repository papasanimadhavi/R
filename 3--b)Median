#Median of the given data.
cat("Enter the vector:")
c <- scan()
for (i in 1:(length(c)-1)){
  for (j in (i+1):length(c)){
  if(c[i]>c[j]){
    temp=c[i]
    c[i]=c[j]
    c[j]=temp
  }
  }
}
cat("Sorted vector is:",c)
k <- length(c)
if(k%%2!=0){
    Median <- c[k/2]
}else{
  Median <- (c[k/2]+c[(k/2)+1])/2
}
cat("Median of the given data is:",Median)



